<h3>Adresses</h3>

<table class="table">
  <tbody>
    <% @addresses.each do |address| %>
      <tr>
        <td><%= address.street %></td>
        <td class="text-right"><%= link_to 'Edit', edit_address_path(address) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p class="text-center">
  <%= link_to 'New Address', new_address_path %>
</p>
